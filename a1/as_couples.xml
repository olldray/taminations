      <move beats="2" hands="left" scaleX="2" scaleY="1.5" select="Extend Left" />
      <move beats="2" hands="both" scaleY=".5" select="Lead Right" />
    </path>
  </tam>

  <tam formation="Two-Faced Tidal Line RH" from="" group="As Couples" parts="" title="As Couples Trade the Wave">
    <path>
      <move beats="4" hands="right" scaleX="3.5" scaleY="2.5" select="Run Right" />
    </path>
    <path>
      <move beats="4" hands="left" scaleX="2.5" scaleY="1.5" select="Run Right" />
    </path>
    <path>
      <move beats="4" hands="right" scaleX="1.5" scaleY="2.5" select="Run Left" />
    </path>
    <path>
      <move beats="4" hands="left" scaleX=".5" scaleY="1.5" select="Run Left" />
    </path>
  </tam>

  <tam formation="Lines Facing Out" from="" group="As Couples" parts="" title="As Couples U-Turn Back">
    <path>
      <move select="Belle Wheel" />
    </path>
    <path>
      <move select="Beau Wheel" />
    </path>
    <path>
      <move reflect="-1" select="Beau Wheel" />
    </path>
    <path>
      <move reflect="-1" select="Belle Wheel" />
    </path>
  </tam>

  <tam formation="Two-Faced Lines RH" from="" group="As Couples" parts="" title="As Couples Walk and Dodge">
    <path numbers="1 8">
      <move hands="right" select="Forward 4" />
    </path>
    <path numbers="2 7">
      <move hands="left" select="Forward 4" />
    </path>
    <path numbers="3 6">
      <move beats="4" hands="gripleft" scaleX="2" scaleY="2" select="Dodge Right" />
    </path>
    <path numbers="4 5">
      <move beats="4" hands="gripright" scaleX="2" scaleY="2" select="Dodge Right" />
    </path>
  </tam>

  <tam formation="Two-Faced Lines RH" from="" group="As Couples" parts="" title="As Couples Zoom">
    <path>
      <move hands="right" select="Forward 4" />
    </path>
    <path>
      <move hands="left" select="Forward 4" />
    </path>
    <path>
      <move hands="left" scaleX="2" scaleY="2" select="Run Left" />
      <move select="Forward 4" />
      <move hands="left" scaleX="2" scaleY="2" select="Run Left" />
    </path>
    <path>
      <move hands="right" scaleX=".5" scaleY=".5" select="Run Left" />
      <move select="Forward 4" />
      <move hands="right" scaleX=".5" scaleY=".5" select="Run Left" />
    </path>
  </tam>


