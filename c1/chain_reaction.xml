<tamination>

  <tam title="Chain Reaction"
       from="Right-Hand Quarter Lines"
       formation="Quarter Lines RH"
       parts="2.5;2" >
    <path numbers="1 8">
      <move select="Forward" />
      <move select="Hinge Right" />
      <move select="Hinge Left" beats="2" />
      <move select="Extend Right" beats="4.5" scaleX="2" scaleY="2" />
    </path>
    <path numbers="2 7">
      <move select="Extend Left"  scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Hinge Left" beats="2" />
      <move select="Hinge Right" hands="right" scaleX="2" scaleY="2" offsetY=".33" />
      <move select="Hinge Right" hands="right" scaleX="2" scaleY="2" offsetX="-.67" offsetY=".67" />
      <move select="Hinge Right" hands="right" scaleX="2" scaleY="2" offsetX="-.33" />
    </path>
    <path numbers="4 5">
      <move select="Extend Left" scaleX="1.5" scaleY=".5" />
      <move select="Hinge Right" offsetX=".5" offsetY="-.5" />
      <move select="Flip Left" beats="2" />
      <move select="Lead Left" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path numbers="3 6">
      <move select="Lead Right" beats="2.5" scaleX="5" scaleY="3" />
      <move select="Run Right" beats="2" />
      <move select="Quarter Right" hands="left" offsetX=".33" offsetY=".33" />
      <move select="Quarter Right" hands="left" offsetX="-.33" offsetY=".33" />
      <move select="Quarter Right" hands="left" offsetX="-.33" offsetY="-.33" />
    </path>
  </tam>

  <tam title="Chain Reaction"
       from="Left-Hand Quarter Lines"
       formation="Quarter Lines LH"
       parts="2.5;2" >
    <path>
      <move select="Extend Left" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Hinge Right" beats="2" />
      <move select="Hinge Left" hands="left" scaleX="2" scaleY="2" offsetY="-.33" />
      <move select="Hinge Left" hands="left" scaleX="2" scaleY="2" offsetX="-.67" offsetY="-.67" />
      <move select="Hinge Left" hands="left" scaleX="2" scaleY="2" offsetX="-.33" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Hinge Left" />
      <move select="Hinge Right" beats="2" />
      <move select="Extend Left" beats="4.5" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" scaleX="1.5" scaleY=".5" />
      <move select="Hinge Left" offsetX=".5" offsetY="-.5" />
      <move select="Run Right" beats="2" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Lead Left" beats="2.5" scaleX="5" scaleY="3" />
      <move select="Flip Left" beats="2" />
      <move select="Quarter Left" hands="right" offsetX=".33" offsetY="-.33" />
      <move select="Quarter Left" hands="right" offsetX="-.33" offsetY="-.33" />
      <move select="Quarter Left" hands="right" offsetX="-.33" offsetY=".33" />
    </path>
  </tam>

  <tam title="Chain Reaction"
       from="Right-Hand Quarter Waves"
       formation="Quarter Wave RH"
       parts="5;3">
    <path>
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Stand" beats="2"/>
      <move select="Eighth Right" beats="1.5" offsetX="1.06" offsetY=".06" />
      <move select="Eighth Right" beats="1.5" offsetX=".707" offsetY="-.793" />
      <move select="Cast Left"/>
    </path>
    <path>
      <move select="Stand" beats="3"/>
      <move select="Quarter Right" beats="2" offsetY="-1"/>
      <move select="Flip Left"/>
      <move select="Lead Left" beats="4.5" scaleX="3" scaleY="3"/>
    </path>
    <path>
      <move select="Lead Right" beats="5" scaleX="5" scaleY="3" />
      <move select="Run Right"/>
      <move select="Cast Left"/>
    </path>
    <path>
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Quarter Left" beats="2" offsetX="-2" offsetY="1"/>
      <move select="Eighth Left" beats="1.5" hands="left" offsetX=".353" offsetY=".647" />
      <move select="Eighth Left" beats="1.5" hands="left" offsetX=".707" offsetY="-.207" />
      <move select="Extend Right" beats="4.5" scaleX="2" scaleY="2"/>
    </path>
  </tam>

  <tam title="Chain Reaction"
       from="Left-Hand Quarter Waves"
       formation="Quarter Wave LH"
       parts="4.5;2" >
    <path>
      <move select="Stand" beats="3"/>
      <move select="Quarter Left" beats="2" offsetX="-2" offsetY="1" />
      <move select="Hinge Left" beats="2" />
      <move select="Extend Right" beats="4.5" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Stand" beats="2"/>
      <move select="Hinge Left" beats="2" />
      <move select="Cast Right" />
    </path>
    <path>
      <move select="Lead Left" beats="4.5" scaleX="5" scaleY="3" />
      <move select="Swing Left" beats="2.5" />
      <move select="Cast Right" />
    </path>
    <path>
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Quarter Right" beats="1.5" offsetY="-1"/>
      <move select="Swing Left" beats="2.5" />
      <move select="Lead Left" beats="4.5" scaleX="3" scaleY="3" />
    </path>
  </tam>

  <tam title="Chain Reaction" from="Right-Hand Two-Faced Line between Mini-Waves"
       parts="5;3">
     <formation>      <dancer gender="boy" x="-3" y="1" angle="180"/>      <dancer gender="girl" x="-3" y="-1" angle="0"/>      <dancer gender="boy" x="0" y="1" angle="0"/>      <dancer gender="girl" x="0" y="3" angle="0"/>    </formation>
    <path numbers="1 8">
      <move select="Stand" beats="3"/>
      <move select="Quarter Left" beats="2" offsetX="-2" offsetY="1" />
      <move select="Hinge Left" beats="3"/>
      <move select="Extend Right" beats="4.5" scaleX="2" scaleY="2" />
    </path>
    <path numbers="2 7">
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Stand" beats="2"/>
      <move select="Hinge Left" beats="3"/>
      <move select="Hinge Right" hands="right" scaleX="2" scaleY="2" offsetY=".33" />
      <move select="Hinge Right" hands="right" scaleX="2" scaleY="2" offsetX="-.67" offsetY=".67" />
      <move select="Hinge Right" hands="right" scaleX="2" scaleY="2" offsetX="-.33" />
    </path>
    <path numbers="4 5">
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Quarter Right" beats="2" offsetY="-1"/>
      <move select="Flip Left"/>
      <move select="Lead Left" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path numbers="3 6">
      <move select="Lead Right" beats="5" scaleX="5" scaleY="3" />
      <move select="Run Right"/>
      <move select="Quarter Right" hands="left" offsetX=".33" offsetY=".33" />
      <move select="Quarter Right" hands="left" offsetX="-.33" offsetY=".33" />
      <move select="Quarter Right" hands="left" offsetX="-.33" offsetY="-.33" />
    </path>
  </tam>

  <tam title="Chain Reaction" from="Left-Hand Two-Faced Line between Mini-Waves"
       parts="5;3">
     <formation>      <dancer gender="boy" x="-3" y="1" angle="0"/>      <dancer gender="girl" x="-3" y="-1" angle="180"/>      <dancer gender="boy" x="0" y="1" angle="180"/>      <dancer gender="girl" x="0" y="3" angle="180"/>    </formation>
    <path>
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Stand" beats="2"/>
      <move select="Eighth Right" beats="1.5" offsetX="1.06" offsetY=".06" />
      <move select="Eighth Right" beats="1.5" offsetX=".707" offsetY="-.793" />
      <move select="Hinge Left" hands="left" scaleX="2" scaleY="2" offsetY="-.33" />
      <move select="Hinge Left" hands="left" scaleX="2" scaleY="2" offsetX="-.67" offsetY="-.67" />
      <move select="Hinge Left" hands="left" scaleX="2" scaleY="2" offsetX="-.33" />
    </path>
    <path>
      <move select="Stand" beats="3"/>
      <move select="Quarter Right" beats="2" offsetY="-1"/>
      <move select="Swing Left"/>
      <move select="Lead Left" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Quarter Left" beats="2" offsetX="-2" offsetY="1" />
      <move select="Eighth Left" beats="1.5" hands="left" offsetX=".353" offsetY=".647" />
      <move select="Eighth Left" beats="1.5" hands="left" offsetX=".707" offsetY="-.207" />
      <move select="Extend Right" beats="4.5" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Lead Left" beats="5" scaleX="5" scaleY="3" />
      <move select="Swing Left"/>
      <move select="Quarter Left" hands="right" offsetX=".33" offsetY="-.33" />
      <move select="Quarter Left" hands="right" offsetX="-.33" offsetY="-.33" />
      <move select="Quarter Left" hands="right" offsetX="-.33" offsetY=".33" />
    </path>
  </tam>

</tamination>
